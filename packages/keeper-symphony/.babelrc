{
  "presets": [
    "@babel/preset-env",
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
      ["module-resolver", {
      "root": ["./extension-app"],
      "alias": {
        "components*": "./components",
        "pages*": "./pages",
        "assets*": "./public/assets",
        "sass*": "./public/sass",
        "reducers*": "./reducers",
        "services*": "./services",
        "utils*": "./utils"
      }
    }]
  ]
}
